{
  "name": "imagedirect-wp-environment",
  "version": "0.0.1",
  "description": "A directory mapped wordpress environment for wp-env",
  "main": "/build/index.js",
  "author": "djcowan",
  "license": "MIT",
  "keywords": [
    "Wordpress",
    "Gutenberg"
  ],
  "scripts": {
    "wp-env": "wp-env",
    "wp-env:go": "wp-env start -- --scripts",
    "wp-env:up-n-go": "wp-env start -- --update --scripts",
    "wp-env:path": "wp-env install-path",
    "wp-env:install-wp-importer": "wp-env run cli wp plugin install wordpress-importer",
    "composer": "wp-env run cli --env-cwd=wp-content/plugins/**plugin** composer"
  },
  "devDependencies": {
    "@wordpress/env": "^10.6.0"
  }
}